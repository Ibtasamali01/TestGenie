{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://backend-production-83c6.up.railway.app/api/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "app/api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "VITE_API_URL": "https://backend-production-83c6.up.railway.app"
  }
} 

